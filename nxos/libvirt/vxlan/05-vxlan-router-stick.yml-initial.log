            - to: 10.1.0.0/16
              via: 172.16.0.2
            - to: 10.2.0.0/24
              via: 172.16.0.2
    SCRIPT
  
    echo -n 'Starting netplan generate ' && date
    netplan generate
    echo -n 'Starting netplan apply ' && date
    nohup netplan apply &
